// program to print the fibonnaci series upto nth term
//0 1 1 2 3 5 8 13 ....
#include<stdio.h>
#include<conio.h>
void main()
{
	int a=0,b=1,c,i,n;
	printf("enter the number of terms you want: ");
	scanf("%d",&n);
		for(i=0;i<=n;i++)
		{
			if(i<=1)
			{
				
				printf("%d\n",i);
			}
			else
			{
					c=a+b;
			        a=b;
		          	b=c;
			        printf("%d\n",c);
			}
		}
	
	getch();
}
